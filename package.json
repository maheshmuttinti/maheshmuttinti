{
  "name": "mobile",
  "version": "0.11.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "delete:watchman": "watchman watch-del-all",
    "android:staging": "ENVFILE=.env.staging react-native run-android",
    "android:production": "ENVFILE=.env.production react-native run-android",
    "android:aab": "react-native run-android --variant=release",
    "ios": "cd ios && pod install && cd ../ && react-native run-ios --simulator='iPhone 12 Pro Max'",
    "ios:14": "cd ios && pod install && cd ../ && react-native run-ios --simulator='iPhone 14'",
    "staging:ios:14": "cd ios && pod install && cd ../ && ENVFILE=.env.staging react-native run-ios --simulator='iPhone 14'",
    "production:ios:14": "cd ios && pod install && cd ../ && ENVFILE=.env.production react-native run-ios --simulator='iPhone 14'",
    "start": "react-native start",
    "reset:cache": "yarn start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "postversion": "react-native-version --amend && git push --follow-tags",
    "np": "np --no-publish --no-tests",
    "staging-release": "branch=$(git rev-parse --symbolic-full-name --abbrev-ref HEAD); if [[ $branch == \"main\" ]]; then npm version patch; else echo \"Error: you are not in main branch. To make Staging release you should be in main branch\"; fi;",
    "production-release": "branch=$(git rev-parse --symbolic-full-name --abbrev-ref HEAD); if [[ $branch == \"releases\" ]]; then npm version minor; else echo \"Error: you are not in releases branch. To make the Production release you should be on the releases branch\"; fi;"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.2.2",
