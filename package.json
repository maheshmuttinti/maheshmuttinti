    "@react-navigation/drawer": "^6.4.3",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reusejs/react-native-component-template": "^0.0.4",
    "@rpldy/native-uploady": "^1.0.0",
    "@shopify/react-native-skia": "^0.1.159",
    "assets": "*",
    "axios": "^0.27.2",
    "digio-sdk-rn": "^0.1.1",
    "dotenv": "^16.0.1",
    "react": "18.1.0",
    "react-native": "0.68.5",
    "react-native-config": "^1.4.5",
    "react-native-document-picker": "^8.1.0",
    "react-native-email-link": "^1.14.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-gifted-charts": "^1.2.41",
